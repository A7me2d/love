{
  "buildCommand": "npm run build",
  "outputDirectory": "docs",
  "framework": "angular",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/docs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0, s-maxage=3600, stale-while-revalidate"
        }
      ]
    }
  ]
}
